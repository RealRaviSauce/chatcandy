{
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/build/$1" 
    },
    { 
      "handle": "filesystem" 
    },
    { 
      "src": "/.*", 
      "dest": "/build/index.html" 
    }
  ]
} 